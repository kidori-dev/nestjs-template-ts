{
  "name": "nestjs",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "typeorm": "env-cmd ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- -d src/database/data-source.ts migration:generate ./src/database/migrations/migrations",
    "migration:create": "npm run typeorm -- migration:create ./src/database/migrations/migrations",
    "migration:run": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:run",
    "migration:revert": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:revert",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test:e2e": "env-cmd jest --verbose --config ./test/jest-e2e.json",
    "prepare": "is-ci || husky install",
    "command": "ts-node -r tsconfig-paths/register ./src/main-cli.ts"
  },
